{
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/server.js -r module-alias/register",
    "dev": "nodemon --exec npx ts-node -r dotenv/config -r tsconfig-paths/register ./server.ts dotenv_config_path=./config/.env"
  },
  "engines": {
    "node": "16.13.0"
  },
  "_moduleAliases": {
    "@root" : "./",
    "@config": "config/*",
    "@models": "models/*",
    "@interfaces": "interfaces/*",
    "@middlewares": "middlewares/*",
    "@schemas": "schema/*",
    "@routes": "routes/*",
    "@utils": "utils/*"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^16.11.10",
    "@types/pg": "^8.6.1",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "pg": "^8.7.1",
    "stripe": "^8.194.0",
    "zod": "^3.11.6"
  }
}
