{
  "scripts": {
    "build": "tsc",
    "start": "node -r module-alias/register ./dist/server.js",
    "dev": "nodemon --exec npx ts-node -r dotenv/config -r tsconfig-paths/register ./server.ts dotenv_config_path=./config/.env"
  },
  "engines": {
    "node": "16.13.0"
  },
  "_moduleAliases": {
    "@config": "dist/config",
    "@models": "dist/models",
    "@interfaces": "dist/interfaces",
    "@middlewares": "dist/middlewares",
    "@schemas": "dist/schema",
    "@routes": "dist/routes",
    "@utils": "dist/utils"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^16.11.10",
    "@types/pg": "^8.6.1",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "pg": "^8.7.1",
    "stripe": "^8.194.0",
    "zod": "^3.11.6"
  }
}
